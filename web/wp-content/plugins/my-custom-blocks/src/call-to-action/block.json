{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "my-custom-blocks/call-to-action",
    "version": "1.0.0",
    "title": "Call to Action",
    "category": "widgets",
    "icon": "megaphone",
    "description": "A customizable call-to-action block with heading, description, and button.",
    "example": {},
    "supports": {
        "html": false,
        "align": [
            "wide",
            "full"
        ]
    },
    "attributes": {
        "heading": {
            "type": "string",
            "default": "Ready to Get Started?"
        },
        "description": {
            "type": "string",
            "default": "Join thousands of satisfied customers today."
        },
        "buttonText": {
            "type": "string",
            "default": "Get Started"
        },
        "buttonUrl": {
            "type": "string",
            "default": ""
        },
        "backgroundColor": {
            "type": "string",
            "default": "#0073aa"
        },
        "textColor": {
            "type": "string",
            "default": "#ffffff"
        },
        "alignment": {
            "type": "string",
            "default": "center"
        }
    },
    "textdomain": "call-to-action-block",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}